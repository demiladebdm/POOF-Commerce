{
  "version": 2,
  "functions": {
    "api-docs": {
      "src": "/api-docs/(.*)",
      "output": "/api-docs/index.html"
    }
  },
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/api-docs/index.html"
    },
    {
      "src": "/api-docs/",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "(.*)",
      "dest": "/index.js"
    }
  ]
}
